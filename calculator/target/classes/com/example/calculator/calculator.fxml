<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.calculator.CalculatorController">
   <children>
      <Label layoutX="240.0" layoutY="73.0" prefHeight="31.0" prefWidth="157.0" text="Calculator " textFill="#11b11e">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <TextField fx:id="firstText" layoutX="69.0" layoutY="152.0" />
      <TextField fx:id="secondText" layoutX="370.0" layoutY="152.0" />
      <Button layoutX="274.0" layoutY="212.0" mnemonicParsing="false" onAction="#addonclick" text="Add" />
      <Label fx:id="ansLabel" layoutX="283.0" layoutY="350.0" text="Ans" />
      <Button layoutX="275.0" layoutY="273.0" mnemonicParsing="false" onAction="#mulonClick" text="mul" />
      <Button layoutX="330.0" layoutY="248.0" mnemonicParsing="false" onAction="#divonClick" text="div" />
      <Button layoutX="222.0" layoutY="248.0" mnemonicParsing="false" onAction="#subonClick" text="sub" />
      <Button layoutX="494.0" layoutY="290.0" mnemonicParsing="false" onAction="#clearonClick" prefHeight="62.0" prefWidth="72.0" text="Clear" />
   </children>
</AnchorPane>
